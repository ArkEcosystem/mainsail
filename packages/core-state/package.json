{
	"name": "@mainsail/core-state",
	"version": "0.0.1",
	"description": "State Management for ARK Core",
	"license": "MIT",
	"contributors": [],
	"main": "distribution/index.js",
	"types": "distribution/index.d.ts",
	"files": [
		"/distribution"
	],
	"scripts": {
		"build": "pnpm run clean && tsc",
