		"@mainsail/kernel": "workspace:*",
		"@mainsail/utils": "workspace:*",
		"immutable": "4.0.0-rc.14",
		"joi": "17.4.2",
		"lmdb": "2.7.11"
	},
	"devDependencies": {
		"import-fresh": "3.3.0",
		"sinon": "^13.0.1",
		"uvu": "^0.5.3"
	},
	"engines": {
		"node": ">=16.x"
	},
	"arkecosystem": {
		"core": {
			"alias": "state"
		}
	}
